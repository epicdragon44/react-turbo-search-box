{
    "name": "react-turbo-search-box",
    "description": " A super-extra, TypeScript-ready, mutex-locked, state-preserving, fuzzy-sorting, pre-and-post-processable search bar for all (some) of your frontend needs.",
    "author": "Daniel Wei",
    "keywords": [
        "react",
        "components",
        "ui",
        "search",
        "mutex",
        "fuzzy",
        "sort",
        "typescript"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/epicdragon44/react-turbo-search-box.git"
    },
    "version": "1.0.3",
    "private": false,
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "fuzzysort": "^2.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.4.2",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "rm -rf dist && NODE_ENV=production babel --extensions .ts,.tsx src --out-dir dist --ignore '**/*.test.tsx' && tsc -p tsconfig.build.json && yarn copy-statics",
        "copy-statics": "cp ./src/pico.css ./dist/pico.css",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.20.2",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "jest": "25.2.2",
        "supertest": "^6.3.3"
    }
}
